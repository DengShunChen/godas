#!/bin/bash
#set -e

#================================================================================
#================================================================================
# Prep SOCA Static B

module use -a /contrib/da/modulefiles
module load jedi
module load nco

if [ ! -d $RUNDIR ]; then mkdir -p $RUNDIR; fi
cd $RUNDIR

echo $SOCA_EXEC
# Create BUMP convolution operator
mkdir -p bump
srun -n 312 $SOCA_EXEC/soca_staticbinit.x ./yaml/static_SocaError_init.yml
